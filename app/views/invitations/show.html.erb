<div class="page-header">
  <%= link_to invitations_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_invitation_path(@invitation), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show invitation</h1>
</div>

<dl class="dl-horizontal">
  <dt>Has plus one:</dt>
  <dd><%= @invitation.has_plus_one %></dd>

  <dt>Guest:</dt>
  <dd><%= @invitation.guest_id %></dd>

</dl>
